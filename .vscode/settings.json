{
  // 自动修复
  "tslint.autoFixOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.tslint": true,
    "source.fixAll.eslint": true
  },
  "fileheader.configObj": {
    "createFileTime": true, //设置为true则为文件新建时候作为date，否则注释生成时间为date
    "prohibitAutoAdd": ["json", "md"], // 禁止.json .md文件，自动添加头部注释
    "dateFormat": "YYYY-MM-DD HH:mm:ss", // 默认格式
    "autoAdd": true, //自动生成注释，老是忘记的同学可以设置
    "atSymbol": "#", // 所有文件的@改为#,默认为@，此为示例。
    "atSymbolObj": {
      "js": "@", // .js文件 去掉@
      "ts": "@",
      "java": "@" // .java文件 @改为#
    },
    "colon": " :", // 所有注释的冒号改为一个空格，默认为": "
    "colonObj": {
      "js": " :", // .js文件 去掉: 留一个空格
      "ts": " :",
      "java": " :" // .java文件 ": "改为$
    },
    "language": {
      "ts": {
        "head": "/**",
        "middle": " * @",
        "end": " */"
      },
      "js": {
        "head": "/**",
        "middle": " * @",
        "end": " */"
      }
    },
    "annotationStr": {
      "head": "/**",
      "middle": " * @",
      "end": " */",
      "use": true //设置自定义注释可用
    },
    "headInsertLine": {
      "php": 2
    },
    "beforeAnnotation": {
      "py": "#!/usr/bin/env python\n# coding=utf-8" // py文件默认，可修改
    }
  },
  "fileheader.cursorMode": {
    "description": "",
    "param ": "",
    "return": ""
  },
  "fileheader.customMade": {
    "Description": "墨抒颖", //文件内容描述
    "Author": "墨抒颖", //编辑人
    "Date": "Do not edit", //时间
    "LastEditTime": "Do not edit",
    "LastEditors": "墨抒颖",
    "Github": "https://github.com/moshuying",
    "Gitee": "https://gitee.com/moshuying",
    "Blogs": "https://blog.csdn.net/qq_34846662",
    "Use":"使用说明"
  },
  "workbench.colorTheme": "Community Material Theme",
  "window.zoomLevel": 0,
  "liveServer.settings.donotShowInfoMsg": true,
  "git.enableSmartCommit": true,
  "editor.minimap.enabled": false,
  "editor.renderWhitespace": "all",
  "editor.renderControlCharacters": true,
  "breadcrumbs.enabled": true,
  "workbench.activityBar.visible": true,
  "eslint.autoFixOnSave": true,
  "eslint.validate": [
    "javascript",
    {
      "language": "vue",
      "autoFix": true
    },
    "html",
    "vue",
    "ts"
  ],
  "explorer.confirmDelete": false,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "sync.gist": "1f4965b7e9c04cbc75cae4a5b04d2f90",
  "files.associations": {
    "*.cjson": "jsonc",
    "*.wxss": "css",
    "*.wxs": "javascript"
  },
  "emmet.includeLanguages": {
    "wxml": "html"
  },
  "minapp-vscode.disableAutoConfig": true,
  "sync.removeExtensions": false,
  "workbench.iconTheme": "vscode-icons",
  "sync.forceUpload": true,
  "python.insidersChannel": "off",
  "editor.tabSize": 2,
  "[json]": {
    "editor.defaultFormatter": "HookyQR.beautify"
  },
  "[html]": {
    "editor.defaultFormatter": "HookyQR.beautify"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "python.formatting.provider": "yapf"
}
